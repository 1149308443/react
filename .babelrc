{
    "plugins":[
        ["@babel/plugin-proposal-decorators", { "legacy": true }],
        "@babel/plugin-proposal-class-properties",
        ["import", {
            "libraryName": "antd",
            "libraryDirectory": "es",
            "style": "css" // `style: true` 会加载 less 文件
        }],
        ["@babel/plugin-transform-runtime"]
    ],
    "presets": [
        "@babel/preset-env", "@babel/preset-react"
    ]
}